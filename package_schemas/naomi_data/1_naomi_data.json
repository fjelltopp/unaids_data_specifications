{
  "scheming_version": 1,
  "dataset_type": "naomi-data-package",
  "name": "Naomi Inputs and Outputs",
  "link_name": "Naomi Inputs and Outputs - Johannesburg Workshop",
  "about": "",
  "about_url": "http://github.com/ckan/ckanext-scheming",
  "add_data_on_create": false,
  "dataset_fields": [
    {
      "field_name": "title",
      "label": "Title (autogenerated):",
      "form_snippet": "text.html",
      "validators": "autogenerate",
      "classes": ["hidden"],
      "template": "{} Naomi Inputs and Outputs {}",
      "template_args": ["geo-location", "year"],
      "template_formatters": ["comma_swap"],
      "display_group": "Naomi Data Package"
    },
    {
      "field_name": "name",
      "label": "URL",
      "form_snippet": "text.html",
      "validators": "autogenerate auto_create_valid_name not_empty unicode name_validator package_name_validator",
      "classes": ["hidden"],
      "template": "{}-naomi-data-{}",
      "template_args": ["geo-location", "year"],
      "template_formatters": ["slugify"],
      "display_group": "Naomi Data Package"
    },
    {
      "field_name": "geo-location",
      "label": "Location",
      "form_placeholder": "Country",
      "preset": "country_list",
      "display_property": "dc:contributor",
      "validators": "not_empty unique_combination",
      "display_group": "Naomi Data Package"
    },
    {
      "field_name": "year",
      "label": "Year",
      "preset": "year",
      "validators": "not_empty unique_combination",
      "display_group": "Naomi Data Package"
    },

    {
      "field_name": "tag_string",
      "label": "Tags",
      "preset": "tag_string_autocomplete",
      "default": "Naomi, ANC, ART, SVY",
      "classes": ["hidden"],
      "display_group": "Naomi Data Package"
    },
    {
      "field_name": "maintainer",
      "label": "Contact Name",
      "preset": "user",
      "user_field": "fullname",
      "form_placeholder": "e.g. Joe Bloggs",
      "display_property": "dc:contributor",
      "validators": "not_empty",
      "display_group": "Naomi Data Package"
    },
    {
      "field_name": "maintainer_email",
      "label": "Contact Email",
      "form_placeholder": "joe@example.com",
      "display_snippet": "email.html",
      "display_email_name_field": "maintainer",
      "validators": "email_validator",
      "display_group": "Naomi Data Package"
    },
    {
      "field_name": "notes",
      "label": "Description/Source",
      "form_snippet": "markdown.html",
      "default": "Inputs and corresponding outputs for the Naomi subnational estimation model.",
      "display_group": "Naomi Data Package"
    },
    {
      "field_name": "license_id",
      "label": "License",
      "form_snippet": "license.html",
      "default": "cc-by-igo",
      "classes": ["hidden"],
      "help_text": "License definitions and additional information can be found at http://opendefinition.org/",
      "display_group": "Naomi Data Package"
    },
    {
      "field_name": "owner_org",
      "label": "Organization",
      "preset": "dataset_organization",
      "private": false,
      "visibility_classes": ["hidden"],
      "display_group": "Naomi Data Package"
    },
    {
      "label": "Data Type",
      "field_name": "type_name",
      "preset": "hidden_value",
      "field_value": "Naomi Data",
      "validators": "unique_combination",
      "display_group": "Naomi Inputs and Outputs"
    }
  ],
  "resources": [{
      "name": "Naomi Spectrum Digest",
      "about": "A ZIP file containing the input and output of the Naomi and Spectrum models",
      "resource_type": "naomi-output-zip",
      "resource_fields": [{
        "field_name": "name",
        "preset": "hidden_value",
        "field_value": "Naomi Output"
      }, {
        "field_name": "format",
        "label": "Format",
        "preset": "resource_format_autocomplete",
        "required": true,
        "default": "zip"
      }, {
        "field_name": "resource_type",
        "preset": "hidden_value",
        "field_value": "naomi-output-zip"
      }]
    }, {
      "name": "Naomi JSON File",
      "about": "The JSON file created by \"File Save\" in the top left corner of the Naomi UI.",
      "resource_type": "naomi-output-json",
      "resource_fields": [{
        "field_name": "name",
        "preset": "hidden_value",
        "field_value": "Naomi Model Options Specification"
      }, {
        "field_name": "format",
        "label": "Format",
        "preset": "resource_format_autocomplete",
        "required": true,
        "default": "json"
      }, {
        "field_name": "resource_type",
        "preset": "hidden_value",
        "field_value": "naomi-output-json"
      }]
    },
    {
      "name": "ANC Data",
      "about": "A validated Naomi ANC dataset.",
      "resource_type": "naomi-anc",
      "resource_fields": [{
        "field_name": "name",
        "preset": "hidden_value",
        "field_value": "ANC Data"
      }, {
        "field_name": "schema",
        "preset": "hidden_value",
        "field_value": "naomi_anc"
      }, {
        "field_name": "resource_type",
        "preset": "hidden_value",
        "field_value": "naomi-anc"
      }]
    },
    {
      "name": "ART Data",
      "about": "A validated Naomi ART dataset",
      "resource_type": "naomi-art",
      "resource_fields": [{
        "field_name": "name",
        "preset": "hidden_value",
        "field_value": "ART Data"
      }, {
        "field_name": "schema",
        "preset": "hidden_value",
        "field_value": "naomi_art"
      }, {
        "field_name": "resource_type",
        "preset": "hidden_value",
        "field_value": "naomi-art"
      }]
    },
    {
      "name": "Survey Data (Johannesburg Workshop)",
      "about": "A validated Naomi survey dataset",
      "resource_type": "naomi-survey",
      "resource_fields": [{
        "field_name": "name",
        "preset": "hidden_value",
        "field_value": "Survey Data"
      }, {
        "field_name": "schema",
        "preset": "hidden_value",
        "field_value": "naomi_survey"
      }, {
        "field_name": "resource_type",
        "preset": "hidden_value",
        "field_value": "naomi-survey"
      }]
    },
    {
      "name": "Population Data",
      "about": "A validated Naomi population dataset",
      "resource_type": "naomi-population",
      "resource_fields": [{
        "field_name": "name",
        "preset": "hidden_value",
        "field_value": "Population Data"
      }, {
        "field_name": "schema",
        "preset": "hidden_value",
        "field_value": "naomi_population"
      }, {
        "field_name": "resource_type",
        "preset": "hidden_value",
        "field_value": "naomi-population"
      }]
    },
    {
      "name": "Geographic Data",
      "about": "A validated Naomi geographic dataset",
      "resource_type": "naomi-geographic",
      "resource_fields": [{
        "field_name": "name",
        "preset": "hidden_value",
        "field_value": "Geographic Data"
      }, {
        "field_name": "schema",
        "preset": "hidden_value",
        "field_value": "naomi_geographic"
      }, {
        "field_name": "format",
        "label": "Format",
        "preset": "resource_format_autocomplete",
        "required": true,
        "default": "GeoJSON"
      }, {
        "field_name": "resource_type",
        "preset": "hidden_value",
        "field_value": "naomi-geographic"
      }]
    },
    {
      "name": "Spectrum File",
      "about": "A spectrum file used to run the Naomi model",
      "resource_type": "naomi-spectrum-file",
      "bulk_upload": true,
      "resource_fields": [{
        "field_name": "name",
        "preset": "hidden_value",
        "field_value": "Spectrum File"
      }, {
        "field_name": "format",
        "label": "Format",
        "preset": "resource_format_autocomplete",
        "required": true,
        "default": "PJNZ"
      }, {
        "field_name": "resource_type",
        "preset": "hidden_value",
        "field_value": "naomi-spectrum-file"
      }, {
          "field_name": "url",
          "label": "URL",
          "preset": "resource_url_upload",
          "upload_label": "File (bulk drag-drop upload available)"
      }]
    }
  ],
  "resource_fields": [{
      "field_name": "url",
      "label": "URL",
      "preset": "resource_url_upload"
    },
    {
      "field_name": "name",
      "label": "Name",
      "form_placeholder": "eg. January 2011 Gold Prices"
    },
    {
      "field_name": "description",
      "label": "Description",
      "form_snippet": "markdown.html",
      "form_placeholder": "Some useful notes about the data"
    },
    {
      "field_name": "format",
      "label": "Format",
      "preset": "resource_format_autocomplete",
      "required": true
    }, {
      "field_name": "restricted",
      "label": "Access Restriction",
      "preset": "restricted_fields",
      "validators": "restricted_json",
      "default_organizations": "unaids",
      "choices": [{
          "value": "restricted",
          "label": "Restricted to specified users and organizations"
        },
        {
          "value": "public",
          "label": "Public"
        }
      ],
      "form_attrs_users": {
        "data-module": "autocomplete",
        "data-module-tags": "",
        "data-module-source": "/api/2/util/user/autocomplete?q=?"
      },
      "form_attrs_orgs": {
        "data-module": "autocomplete",
        "data-module-tags": "",
        "data-module-source": "/api/2/util/organization/autocomplete?q=?"
      }
    }
  ]
}
